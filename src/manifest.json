{
    "$schema": "https://raw.githubusercontent.com/cezaraugusto/chrome-extension-manifest-json-schema/main/manifest/manifest.schema.v3.json",
    "name": "ZetBot-Cookie",
    "version": "1.3.0",
    "description": "Obtenez vos Cookies Facebook en un Clique avec ZetBot-Cookie.",
    "permissions": [
        "cookies",
        "clipboardRead"
    ],
    "host_permissions": [
        "https://*.facebook.com/"
    ],
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "assets/logo/logo.png",
            "32": "assets/logo/logo.png",
            "48": "assets/logo/logo.png",
            "64": "assets/logo/logo.png",
            "128": "assets/logo/logo.png"
        }
    },
    "icons": {
        "16": "assets/logo/logo.png",
        "32": "assets/logo/logo.png",
        "48": "assets/logo/logo.png",
        "64": "assets/logo/logo.png",
        "128": "assets/logo/logo.png"
    },
    "manifest_version": 3,
    "browser_specific_settings": {
		"gecko": {
			"id": "{91e2b93a-c014-4d6f-a384-a015e85f0727}"
		}
	}
}
